    </script>


    <script type="text/javascript">
        $(document).ready(function(){

            $("#placeOrder").on('click', function(){
                var formData = $('#orderForm').serializeArray();
                var dataObj = {};
                
                $(formData).each(function(i, field){
                    dataObj[field.name] = field.value;
                });

                $("#orderForm :input").prop("disabled", true);
                $('#placeorder-spinner').removeClass('hide').addClass('show');
                $('#placeorder-btn-label').removeClass('show').addClass('hide');

                $.ajax({
                    method:"post",
                    url:"/productOrder",
                    data:dataObj,
                    success:function(response){
                        if(response['success'] == 1){
                            $('#placeorder-success-msg-data').html(response['msg']);
                            $('#placeorder-success-msg').removeClass('hide').addClass('show');
                            $(".order-field-empty").val('');
                            $('#quantity').val('1');

                            setTimeout(function(){
                                $('#placeorder-success-msg').removeClass('show').addClass('hide');
                            }, 6000);
                            //grecaptcha.reset();
                            grecaptcha.reset($('#orderForm .reset-captcha').data('grecapid'));
                        }
                        else{
                            grecaptcha.reset($('#orderForm .reset-captcha').data('grecapid'));

                            $('#placeorder-error-msg-data').html(response['msg']);
                            $('#placeorder-error-msg').removeClass('hide').addClass('show');
                        }

                        $("#orderForm :input").prop("disabled", false);
                        $('#placeorder-spinner').removeClass('show').addClass('hide');
                        $('#placeorder-btn-label').removeClass('hide').addClass('show');
                    },
                    error:function(response){
                        $('#placeorder-error-msg-data').html(response['msg']);
                        $('#placeorder-error-msg').removeClass('hide').addClass('show');

                        $("#orderForm :input").prop("disabled", false);
                        $('#placeorder-spinner').removeClass('show').addClass('hide');
                        $('#placeorder-btn-label').removeClass('hide').addClass('show');
                    }
                })

            });

        });
    </script>

    
    <script type="text/javascript">
    if ('serviceWorker' in navigator) {
        try {
            navigator.serviceWorker.register("https://realtouchelectronics.websites.co.in/service.worker.js?v=532968225").then(function(
                registration) {
                // Registration was successful
                console.log('Laravel PWA: ServiceWorker registration successful with scope: ', registration
                    .scope);
            }, function(err) {
                // registration failed :(
                console.log('Laravel PWA: ServiceWorker registration failed: ', err);
            });
        } catch (error) {
            console.log('service worker failed');
        }
    }
</script>

